{
  "name": "react-typescript-boilerplate",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "start": "webpack-dev-server --config config/webpack/webpack.dev.js",
    "build:clean": "rimraf ./dist",
    "prebuild": "npm run build:clean",
    "build": "webpack --config config/webpack/webpack.prod.js",
    "check-types": "tsc",
    "lint": "tslint --project tsconfig.json",
    "prettier": "prettier --write --no-editorconfig"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check-types && npm run test"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "npm run prettier",
      "git add"
    ],
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "author": "Hai Phan",
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "babel-loader": "8.0.4",
    "cross-env": "5.2.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.3",
    "image-webpack-loader": "4.4.0",
    "json-loader": "0.5.7",
    "lint-staged": "8.0.4",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.1.3",
    "url-loader": "1.1.2",
    "webpack": "4.22.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9",
    "webpack-merge": "4.1.4"
  },
  "dependencies": {
    "react": "16.5.2",
    "react-dom": "16.5.2"
  }
}
